    "@types/morgan": "^1.9.4",
    "@types/node": "^17.0.12",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@types/uuid": "^9.0.0",
    "dotenv": "^16.0.3",
    "eslint": "7.32.0",
