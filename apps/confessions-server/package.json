    "@prisma/client": "^4.12.0",
    "@types/lodash": "^4.14.191",
    "@types/pg": "^8.6.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "libhoney": "^4.0.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "next": "^13.1.1",
    "p-queue": "6",
    "pg": "^8.8.0",
    "postgres-migrations": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0"
