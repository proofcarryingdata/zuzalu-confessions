  },
  "devDependencies": {
    "@pcd/eslint-config-custom": "0.4.0",
    "prettier": "latest",
    "prettier-plugin-organize-imports": "^3.2.2"
  },
  "engines": {
    "node": "^18.0.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.0",
    "turbo": "^1.8.5"
  },
  "packageManager": "yarn@1.22.19"
}